{
    "name": "nextjs-boilerplate",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start:dev": "env-cmd -f ./src/env/.env.development next dev",
        "build:prod": "env-cmd -f ./src/env/.env.production next build",
        "start:prod": "env-cmd -f ./src/env/.env.production  next start",
        "lint": "next lint",
        "prepare": "husky",
        "format": "prettier --check --ignore-path .gitignore .",
        "format:fix": "prettier --write --ignore-path .gitignore ."
    },
    "dependencies": {
        "@tanstack/react-query": "^5.29.2",
        "@trpc/client": "^11.0.0-rc.346",
        "@trpc/next": "^11.0.0-rc.346",
        "@trpc/react-query": "^11.0.0-rc.346",
        "@trpc/server": "^11.0.0-rc.346",
        "dotenv": "^16.4.5",
        "env-cmd": "^10.1.0",
        "next": "14.2.2",
        "react": "^18",
        "react-dom": "^18",
        "superjson": "^2.2.1",
        "zod": "^3.22.5"
    },
    "devDependencies": {
        "@svgr/webpack": "^8.1.0",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "daisyui": "^4.10.2",
        "eslint": "^8",
        "eslint-config-next": "14.2.2",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^9.0.11",
        "postcss": "^8",
        "prettier": "^3.2.5",
        "prisma": "^5.12.1",
        "tailwindcss": "^3.4.1",
        "typescript": "^5"
    }
}
